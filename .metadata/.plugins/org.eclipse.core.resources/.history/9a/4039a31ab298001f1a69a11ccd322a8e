#include <main.h>
#include <assert.h>

/*
   Création d'un trame d'une mesure de SCAN normale d'exemple

	S de 0
	quality de 10
	angle de 10 °
	distance de 10 mm
 */

frame_t frame_under_test = {
		0xA5,
		0x5A,
		0x2A,
		0x01,
		0x05,
		0x28,
		0x00
};

void do_all_tests()
{
	do_lidar_distance_test();
}

void do_lidar_distance_test()
{
	assert(lidar_distance(frame_under_test) == 10 && "Distance equal to 10");
}


